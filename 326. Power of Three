public class Solution {
    public boolean isPowerOfThree(int n) {
        double ans=Math.log10(n)/Math.log10(3);//n若是3的指數則ans必為整數
        if(ans-(int)ans==0.0) //
          return true;
        else 
          return false;
    }
}
